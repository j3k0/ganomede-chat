  },
  "author": "Jean-Christophe Hoelt <hoelt@fovea.cc>",
  "license": "Private",
  "dependencies": {
    "async": "^1.5.0",
    "authdb": "^0.2.0",
    "bunyan": "^1.8.12",
    "cd": "^0.3.3",
    "forever": "^0.15.1",
    "lodash": "^3.10.1",
    "newrelic": "^1.20.2",
    "node-statsd": "^0.1.1",
    "redis": "^3.0.2",
    "restify": "^8.5.1",
    "restify-clients": "^4.0.0",
    "restify-errors": "^8.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@mayachit/mocha-pretty-bunyan": "^1.0.1",
    "@types/bunyan": "^1.8.6",
