{
  "name": "chat",
  "version": "1.1.0",
  "api": "chat/v1",
  "description": "chat micro service",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "make check",
    "test": "make test",
    "testw": "nodemon --ext js,coffee --watch index.js --watch config.js --watch newrelic.js --watch src/ --watch tests/ -x 'npm test'"
  },
  "author": "Jean-Christophe Hoelt <hoelt@fovea.cc>",
  "license": "Private",
  "dependencies": {
    "async": "^1.5.0",
    "authdb": "^0.2.0",
    "bunyan": "^1.2.3",
    "coffee-script": "^1.10.0",
    "forever": "^0.15.1",
    "ganomede-helpers": "^1.0.1",
    "lodash": "^3.10.1",
    "newrelic": "^1.20.2",
    "redis": "^2.2.5",
    "restify": "^5.0.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "coffeelint": "^1.13.0",
    "eslint": "^1.7.2",
    "expect.js": "^0.3.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  }
}
