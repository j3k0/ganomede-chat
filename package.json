{
  "name": "chat",
  "version": "1.2.1",
  "api": "chat/v1",
  "description": "chat micro service",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node -r source-map-support/register build/src/index.js",
    "build": "tsc",
    "buildw": "tsc -w",
    "debug": "API_SECRET=42 nodemon --exec \" ts-node src/index.ts\" -w src -e ts | bunyan -o short --color",
