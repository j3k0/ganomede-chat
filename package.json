    "@types/restify": "^8.4.2",
    "@types/restify-errors": "^4.3.3",
    "blanket": "^1.1.6",
    "eslint": "^4.3.0",
    "expect.js": "^0.3.1",
    "fakeredis": "^2.0.0",
    "mocha": "^2.3.3",
    "nodemon": "^2.0.4",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0",
    "typescript": "^3.9.3"
  }
